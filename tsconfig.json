{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "jsx": "react",
        "lib": [
            "dom",
            "es5",
            "es2015"
        ],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "pretty": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es6"
    },
    "exclude": [
        "node_modules"
    ]
}
